   "source": [
    "Write a Python program to count the occurrences of each word in a string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "431092d6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'hello': 2, 'world': 1}\n"
     ]
    }
   ],
   "source": [
    "def word_count(s):\n",
    "    words = s.split()\n",
    "    count = {}\n",
    "    for word in words:\n",
    "        count[word] = count.get(word, 0) + 1\n",
    "    return count\n",
    "\n",
    "# Example usage\n",
    "print(word_count(\"hello world hello\"))"
   ]
  }
 ],
